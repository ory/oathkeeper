  "config": {
    "prettierTarget": "(contrib/**/*.{js,md,json,yml})|*.md"
  },
  "devDependencies": {
    "doctoc": "^2.0.0",
    "ory-prettier-styles": "^1.1.2",
    "prettier": "^2.2.1"
  },
  "dependencies": {
    "@openapitools/openapi-generator-cli": "^2.4.26"
  }
}
